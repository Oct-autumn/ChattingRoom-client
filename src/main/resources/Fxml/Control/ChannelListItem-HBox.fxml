<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<fx:root type="HBox" alignment="CENTER_LEFT" focusTraversable="true" maxHeight="-Infinity"
         maxWidth="1.7976931348623157E308"
         minHeight="-Infinity" prefHeight="40.0" prefWidth="300.0" styleClass="h-box-channel-list-item-normal"
         stylesheets="@../../Css/ChannelListItem-HBox.css" xmlns="http://javafx.com/javafx/18"
         xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="2.0" left="3.0" right="3.0" top="2.0"/>
    </padding>
    <VBox alignment="CENTER_LEFT">
        <Text fx:id="textChannelName" strokeType="OUTSIDE" strokeWidth="0.0"
              styleClass="text-channel-name-normal" text="Example"/>
        <Text fx:id="textChannelInfo" strokeType="OUTSIDE" strokeWidth="0.0"
              styleClass="text-channel-info-normal" text="Example"/>
    </VBox>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
        <Separator orientation="VERTICAL" prefHeight="200.0">
            <HBox.margin>
                <Insets bottom="5.0" top="5.0"/>
            </HBox.margin>
        </Separator>
        <Button fx:id="channelOperation" mnemonicParsing="false">
            <styleClass>
                <String fx:value="button-channel-operation-base"/>
                <String fx:value="button-channel-operation-normal"/>
            </styleClass>
        </Button>
    </HBox>
</fx:root>